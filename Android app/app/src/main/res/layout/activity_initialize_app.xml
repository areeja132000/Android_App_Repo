<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InitializeApp">

    <Button
        android:id="@+id/first_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginEnd="140dp"
        android:layout_marginBottom="50dp"
        android:text="@string/first_admin"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@+id/employName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.397"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/first_employee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginEnd="140dp"
        android:text="@string/first_employ"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@+id/backButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="116dp"
        android:text="@string/initialization_title"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/adminName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/backButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/back"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/employPass"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="15dp"
        android:hint="@string/create_acc_password"
        android:textSize="8sp"
        app:layout_constraintBottom_toTopOf="@+id/first_employee"
        tools:layout_editor_absoluteX="0dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/employAddress"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/create_acc_address"
        android:textSize="8sp"
        app:layout_constraintBottom_toTopOf="@+id/employPass"
        tools:layout_editor_absoluteX="0dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/employAge"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/create_acc_age"
        android:textSize="8sp"
        app:layout_constraintBottom_toTopOf="@+id/employAddress"
        tools:layout_editor_absoluteX="0dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/employName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/create_acc_name"
        android:textSize="8sp"
        app:layout_constraintBottom_toTopOf="@+id/employAge"
        tools:layout_editor_absoluteX="0dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/adminAddress"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/create_acc_address"
        android:textSize="8sp"
        app:layout_constraintBottom_toTopOf="@+id/adminPassword"
        tools:layout_editor_absoluteX="21dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/adminName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/create_acc_name"
        android:textSize="8sp"
        app:layout_constraintBottom_toTopOf="@+id/adminAge"
        tools:layout_editor_absoluteX="0dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/adminPassword"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="49dp"
        android:hint="@string/create_acc_password"
        android:textSize="8sp"
        app:layout_constraintBottom_toTopOf="@+id/first_admin"
        tools:layout_editor_absoluteX="0dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/adminAge"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/create_acc_age"
        android:textSize="8sp"
        app:layout_constraintBottom_toTopOf="@+id/adminAddress"
        tools:layout_editor_absoluteX="0dp" />
</androidx.constraintlayout.widget.ConstraintLayout>